        <div class="content content1 active">
          <div class="head-wrpr">
            <div class="">
              <h3 class="main-title">Categories</h3>
              <hr>
            </div>
            <div class="">
              {% if collection.all_tags.size > 0 %}
                <ul class="tag-filters">
                  {% for tag in collection.all_tags %}
