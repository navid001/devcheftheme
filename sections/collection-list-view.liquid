              <div class="loading-overlay gradient"></div>

              <ul
                id="product-grid"
                data-id="{{ section.id }}"
                class="
                  grid product-grid grid--{{ section.settings.columns_mobile }}-col-tablet-down
                  grid--{{ section.settings.columns_desktop }}-col-desktop
                "
              >
                {%- for product in collection.products -%}
                  {% assign lazy_load = false %}
                  {%- if forloop.index > 2 -%}
                    {%- assign lazy_load = true -%}
                  {%- endif -%}
                  <li class="grid__item">
                    <div id="product_list_items">
                      <div data-list-local="Yes" style="display:none;">
                        <p>data list local yes</p>
                        {% assign only_local = product.metafields.custom.only_local %}
                        {% unless only_local == 'true' %}
                          <p>only local not true</p>

                          {% render 'list-product',
                            card_product: product,
                            media_aspect_ratio: section.settings.image_ratio,
                            show_secondary_image: section.settings.show_secondary_image,
                            show_vendor: section.settings.show_vendor,
                            show_rating: section.settings.show_rating,
                            lazy_load: lazy_load,
                            show_quick_add: section.settings.enable_quick_add,
                            section_id: section.id
                          %}
                        {% endunless %}
                      </div>
                      <div data-list-local="No" style="display:none;">
                        <p>data list local no</p>

                        {% render 'list-product',
                          card_product: product,
                          media_aspect_ratio: section.settings.image_ratio,
                          show_secondary_image: section.settings.show_secondary_image,
                          show_vendor: section.settings.show_vendor,
                          show_rating: section.settings.show_rating,
                          lazy_load: lazy_load,
                          show_quick_add: section.settings.enable_quick_add,
                          section_id: section.id
                        %}
                      </div>
                    </div>
                  </li>
                {%- endfor -%}
              </ul>

              {%- if paginate.pages > 1 -%}
                {% render 'pagination', paginate: paginate, anchor: '' %}
              {%- endif -%}
            </div>
          {%- endif -%}
        </div>

        <div class="content content2">
          {% render 'testimonial-slider' %}

          {% assign nav_range = section.settings.testimonials_limit %}
          {% style %}
            @media only screen and ( min-width: 1201px ) {
            	{% if nav_range == 2 %}
