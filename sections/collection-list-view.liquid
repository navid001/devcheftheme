                      <span class="testimonial__author-company">{{ block.settings.company }}</span>
                    </div>
                  </div>
                {% endfor %}
              </div>
            </div>
          -->
        </div>
        <div class="content content3">Content 3</div>
      </div>

      <aside
        aria-labelledby="verticalTitle"
        class="facets-wrapper right-sidebar {% unless section.settings.enable_filtering %} facets-wrapper--no-filters{% endunless %}{% if section.settings.filter_type != 'vertical' %} page-width{% endif %}"
        id="main-collection-filters"
        data-id="{{ section.id }}"
      >
        <div class="right-side">
          {% render 'best-selling-slider', product: product %}
          {% if section.settings.sale_product != blank %}
            {% render 'sale-product', product: section.settings.sale_product %}
          {% endif %}
        </div>
      </aside>
    </div>
  {%- endpaginate -%}
</div>

