                  {% for tag in collection.all_tags %}
                    {% if tag contains 'filter_' %}
                      {% if current_tags contains tag %}
                        {% if tag contains 'filter_Preset' %}
                        {% else %}
                          <li class="tag-filters__item active">
                            {{ tag | replace: 'filter_', '' | link_to_remove_tag: tag }}
                          </li>
                        {% endif %}

                      {% else %}
                        {% if tag contains 'filter_Preset' %}
                        {% else %}
                          <li class="tag-filters__item t_{{ tag }}">
                            {{ tag | replace: 'filter_', '' | link_to_tag: tag }}
                          </li>
                        {% endif %}
                      {% endif %}
                    {% endif %}
                  {% endfor %}
                </ul>
              {% endif %}
