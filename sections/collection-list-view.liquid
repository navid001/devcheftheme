{%- endstyle -%}
{% assign vendor_supported_zip_codes_in_list = null %}
{% if collection.title == 'Green' %}
  {% assign vendor_name = 'Green' %}
{% else %}
  {% assign vendor_name = collection.title %}
{% endif %}
{% for collection in collections %}
  {% if collection.title == vendor_name %}
    {% assign zip_codes = collection.metafields.custom.zip_codes %}
    {% if zip_codes %}
      {%- assign vendor_supported_zip_codes_in_list = zip_codes.value | parse_json | default: '[]' -%}
      {% break %}
    {% endif %}
  {% endif %}
{% endfor %}

<script>
  document.addEventListener('DOMContentLoaded', function() {

  console.log("List is running")
          var userZipCode = localStorage.getItem('userZipCode');
          console.log("User in list",userZipCode);

        // Function to check if user's zip code is in the vendor's supported zones
        function isLocalDeliveryAvailable(userZip, supportedZips) {
          if (!userZip || !supportedZips || supportedZips.length === 0) {
            return false;
          }
          return supportedZips.includes(userZip);
        }

        // Get user's zip code from data attribute
        var vendorName="{{vendor_name}}";
        if (vendorName){
          console.log("vendorName in list",vendorName);
          var vendor_supported_zip_codes_in_list={{vendor_supported_zip_codes_in_list}};

        // Parse the vendor supported zip codes from Liquid
        var vendorSupportedZipCodes = {% if vendor_supported_zip_codes_in_list %}{{ vendor_supported_zip_codes_in_list | json }}{% else %}[]{% endif %};


        // Determine if local delivery is available
        var isLocalDelivery = isLocalDeliveryAvailable(userZipCode, vendorSupportedZipCodes);
      console.log(isLocalDelivery,"isLocalDelivery in list");
    localValue = isLocalDelivery ? 'Yes' : 'No';
          };
    console.log(localValue,"localValue in list");
  localPriceElements = document.querySelectorAll(`[data-list-local="${localValue}"]`);
  console.log("localPriceElements",localPriceElements);
  // Show all matching elements
  localPriceElements.forEach(element => {
    element.style.display="block";
    console.log("Trying");
  });
  });
</script>
<div class="section-{{ section.id }}-padding restaurant-page">
  <div class="resturant-info page-width">
    <div class="info-wrpr">
      <img src=" {{ collection.featured_image | img_url: 'thumbnail' }}">

      <h3>{{ collection.title }}</h3>
      {% comment %}<p>{{ collection.metafields.custom.restaurent_time }}</p>{% endcomment %}
      {% assign vendor_name = collection.title %}
      {% render 'delivery-timer', vendor_name: vendor_name %}
    </div>
    <div class="d-none">{{ collection.metafields.custom.announcement_text }}</div>
  </div>
  {%- paginate collection.products by section.settings.products_per_page -%}
    {% comment %} Sort is the first tabbable element when filter type is vertical {% endcomment %}
    {%- if section.settings.enable_sorting and section.settings.filter_type == 'vertical' -%}
      <facet-filters-form class="facets facets-vertical-sort page-width small-hide no-js-hidden">
        <form class="facets-vertical-form" id="FacetSortForm">
          <div class="facet-filters sorting caption">
            <div class="facet-filters__field">
              <h2 class="facet-filters__label caption-large text-body">
                <label for="SortBy">{{ 'products.facets.sort_by_label' | t }}</label>
              </h2>
              <div class="select">
                {%- assign sort_by = collection.sort_by | default: collection.default_sort_by -%}
                <select
                  name="sort_by"
                  class="facet-filters__sort select__select caption-large"
                  id="SortBy"
                  aria-describedby="a11y-refresh-page-message"
                >
                  {%- for option in collection.sort_options -%}
                    <option
                      value="{{ option.value | escape }}"
                      {% if option.value == sort_by %}
                        selected="selected"
                      {% endif %}
                    >
                      {{ option.name | escape }}
                    </option>
                  {%- endfor -%}
                </select>
                {% render 'icon-caret' %}
              </div>
            </div>
            <noscript>
              <button type="submit" class="facets__button-no-js button button--secondary">
                {{ 'products.facets.sort_button' | t }}
              </button>
            </noscript>
          </div>

          <div class="product-count-vertical light" role="status">
            <h2 class="product-count__text text-body">
              <span id="ProductCountDesktop">
                {%- if collection.results_count -%}
                  {{
                    'templates.search.results_with_count'
                    | t: terms: collection.terms, count: collection.results_count
                  }}
                {%- elsif collection.products_count == collection.all_products_count -%}
                  {{ 'products.facets.product_count_simple' | t: count: collection.products_count }}
                {%- else -%}
                  {{
                    'products.facets.product_count'
                    | t: product_count: collection.products_count, count: collection.all_products_count
                  }}
                {%- endif -%}
              </span>
            </h2>
            <div class="loading-overlay__spinner">
              <svg
                aria-hidden="true"
                focusable="false"
                class="spinner"
                viewBox="0 0 66 66"
                xmlns="http://www.w3.org/2000/svg"
              >
                <circle class="path" fill="none" stroke-width="6" cx="33" cy="33" r="30"></circle>
              </svg>
            </div>
          </div>
        </form>
      </facet-filters-form>
    {%- endif -%}

